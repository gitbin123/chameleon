<view class="{{ disabled?'cml-switch-disabled':'cml-switch' }}  cml-base cml-view cml-0acedec6">
  <switch checked="{{ checked }}" disabled="{{ disabled }}" onChange="_cmlEventProxy" data-eventchange="switchChange" color="{{ opencolor }}" class=" cml-base cml-switch cml-0acedec6" />
    <text class="cml-switch-label  cml-base cml-text cml-0acedec6" a:if="{{ label }}">{{label}}</text>
</view>